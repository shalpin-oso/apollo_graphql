type Product @key(fields: "id") {
  id: String!
  name: String!
}

type Query {
  touches: [Touch!]!
}

type SearchQueryAndProducts @key(fields: "query") {
  products: [Product!]! @external
  query: String!
}

type Touch {
  id: String!
  name: String!
  searchQueryAndProducts: SearchQueryAndProducts!
}